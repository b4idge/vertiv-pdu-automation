# Copy this file to .env and fill in your actual values
# .env file will be ignored by git for security

# Vertiv PDU Credentials
VERTIV_USERNAME=your_username_here
VERTIV_PASSWORD=your_password_here
VERTIV_API_KEY=your_base64_encoded_credentials_here
VERTIV_API_TOKEN=your_api_token_here

# NetBox Configuration
NETBOX_API_TOKEN=your_netbox_token_here
NETBOX_API_URL=https://your-netbox-url.com/api

# Nornir Filter (optional)
NORNIR_FILTER_DESCRIPTION=your_device_filter_here